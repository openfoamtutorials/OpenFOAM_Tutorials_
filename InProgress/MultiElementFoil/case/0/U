FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (10 0 0);

boundaryField
{
    frontAndBack
    {
        type            empty;
    }
    tunnel
    {
        type            slip;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform (0 0 0);
        value           uniform (10 0 0);
    }
    inlet
    {
        type            fixedValue;
        value           uniform (10 0 0);
    }
    plate
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
}


