FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 20;

boundaryField
{
    rotor
    {
        type            epsilonWallFunction;
        value           nonuniform 0();
    }
    stator
    {
        type            epsilonWallFunction;
        value           uniform 20;
    }
    AMI1
    {
        type            cyclicAMI;
        value           nonuniform 0();
    }
    AMI2
    {
        type            cyclicAMI;
        value           uniform 20;
    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform 20;
    }
}


